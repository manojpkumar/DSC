        "from collections import Counter \n",
        "input()\n",
        "pairs=0\n",
        "gloves=Counter(map(int,input().strip().split()))\n",
        "for i in gloves:\n",
        "  pairs +=gloves[i]//2\n",
        "print(pairs)  \n"
